<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".IntentRespuestaActivity">

    <Button
            android:text="Enviar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btn_intent_resp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="340dp" app:layout_constraintHorizontal_bias="0.501"/>
    <Button
            android:text="Enviar Respuesta Propia"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btn_resp_prop"
            android:layout_marginBottom="268dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/btn_intent_resp"
            app:layout_constraintStart_toStartOf="@+id/btn_intent_resp" app:layout_constraintHorizontal_bias="0.508"/>
</androidx.constraintlayout.widget.ConstraintLayout>