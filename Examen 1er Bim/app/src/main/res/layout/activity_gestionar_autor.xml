<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".GestionarAutor" android:id="@+id/gestionarAutor">
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/input_nombre2" android:layout_marginBottom="32dp"
            app:layout_constraintBottom_toTopOf="@+id/input_apell2" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/input_apell2"
            app:layout_constraintEnd_toEndOf="@+id/input_nombre2"
            app:layout_constraintStart_toStartOf="@+id/input_nombre2" android:layout_marginBottom="28dp"
            app:layout_constraintBottom_toTopOf="@+id/input_fech2" app:layout_constraintHorizontal_bias="0.0"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/input_fech2"
            app:layout_constraintEnd_toEndOf="@+id/input_apell2"
            app:layout_constraintStart_toStartOf="@+id/input_apell2" android:layout_marginBottom="40dp"
            app:layout_constraintBottom_toTopOf="@+id/input_ecu2" app:layout_constraintHorizontal_bias="0.0"/>
    <CheckBox
            android:text="CheckBox"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/input_ecu2" app:layout_constraintStart_toStartOf="@+id/input_fech2"
            app:layout_constraintEnd_toEndOf="@+id/input_fech2"
            app:layout_constraintHorizontal_bias="0.495"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/input_numLib2"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:id="@+id/input_numLib2" app:layout_constraintStart_toStartOf="@+id/input_ecu2"
            app:layout_constraintEnd_toEndOf="@+id/input_ecu2" app:layout_constraintHorizontal_bias="0.594"
            app:layout_constraintBottom_toTopOf="@+id/btn_elim" android:layout_marginBottom="52dp"/>
    <Button
            android:text="El"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btn_elim" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="52dp"
            android:layout_marginBottom="36dp" app:layout_constraintBottom_toTopOf="@+id/btn_gest_lib"
    />
    <Button
            android:text="act"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btn_act" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/btn_elim" android:layout_marginStart="8dp"
            android:layout_marginBottom="36dp" app:layout_constraintBottom_toTopOf="@+id/btn_cre_lib"
            app:layout_constraintHorizontal_bias="0.502"
    />
    <Button
            android:text="gest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btn_gest_lib" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="52dp"
            android:layout_marginBottom="32dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <Button
            android:text="cre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btn_cre_lib" app:layout_constraintStart_toEndOf="@+id/btn_gest_lib"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginBottom="32dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.502"/>
</android.support.constraint.ConstraintLayout>